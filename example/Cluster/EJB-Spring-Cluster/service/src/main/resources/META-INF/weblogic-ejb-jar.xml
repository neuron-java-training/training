<?xml version="1.0" encoding="UTF-8"?>
<wls:weblogic-ejb-jar
	xmlns:wls="http://xmlns.oracle.com/weblogic/weblogic-ejb-jar"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd http://xmlns.oracle.com/weblogic/weblogic-ejb-jar http://xmlns.oracle.com/weblogic/weblogic-ejb-jar/1.4/weblogic-ejb-jar.xsd">
	<!--weblogic-version:12.1.2 -->
	<wls:weblogic-enterprise-bean>
		<wls:ejb-name>AsyncProcessorBean</wls:ejb-name>
        <wls:singleton-session-descriptor>
            <wls:singleton-clustering>
                <wls:use-serverside-stubs>true</wls:use-serverside-stubs>
            </wls:singleton-clustering>
        </wls:singleton-session-descriptor>
        <wls:enable-call-by-reference>true</wls:enable-call-by-reference>
	</wls:weblogic-enterprise-bean>
	<wls:weblogic-enterprise-bean>
		<wls:ejb-name>RoleConverter</wls:ejb-name>
		<wls:singleton-session-descriptor></wls:singleton-session-descriptor>
        <wls:enable-call-by-reference>true</wls:enable-call-by-reference>
	</wls:weblogic-enterprise-bean>
	<wls:weblogic-enterprise-bean>
		<wls:ejb-name>RoleService</wls:ejb-name>
		<wls:stateless-session-descriptor></wls:stateless-session-descriptor>
        <wls:enable-call-by-reference>true</wls:enable-call-by-reference>
	</wls:weblogic-enterprise-bean>
	<wls:weblogic-enterprise-bean>
		<wls:ejb-name>TimerBean</wls:ejb-name>
		<wls:singleton-session-descriptor></wls:singleton-session-descriptor>
        <wls:enable-call-by-reference>true</wls:enable-call-by-reference>
	</wls:weblogic-enterprise-bean>
	<wls:weblogic-enterprise-bean>
		<wls:ejb-name>UserConverter</wls:ejb-name>
		<wls:singleton-session-descriptor></wls:singleton-session-descriptor>
        <wls:enable-call-by-reference>true</wls:enable-call-by-reference>
	</wls:weblogic-enterprise-bean>
	<wls:weblogic-enterprise-bean>
		<wls:ejb-name>UserFacade</wls:ejb-name>
		<wls:stateless-session-descriptor></wls:stateless-session-descriptor>
        <wls:enable-call-by-reference>true</wls:enable-call-by-reference>
	</wls:weblogic-enterprise-bean>
    <wls:weblogic-enterprise-bean>
        <wls:ejb-name>UserService</wls:ejb-name>
        <wls:stateless-session-descriptor></wls:stateless-session-descriptor>
        <wls:enable-call-by-reference>true</wls:enable-call-by-reference>
    </wls:weblogic-enterprise-bean>
</wls:weblogic-ejb-jar>